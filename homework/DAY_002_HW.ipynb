    "import cv2\n",
    "img = cv2.imread('lena.png', cv2.IMREAD_GRAYSCALE)# 以灰階的格式來讀取圖片\n",
    "cv2.imshow('rgb2',img)\n",
    "img = cv2.imread('lena.png', cv2.IMREAD_UNCHANGED)# 載入圖片中所有 channel\n",
    "cv2.imshow('rgb3',img)\n",
    "cv2.waitKey(0)# 按下任意鍵則關閉所有視窗\n",
    "cv2.destroyAllWindows()"
   ]
